  <PropertyGroup>
    <EnableSourceLink>True</EnableSourceLink>
  </PropertyGroup>

  <Import Project="Sdk.props" Sdk="Microsoft.DotNet.Arcade.Sdk" />
  <Import Project="eng\targets\Settings.props" />

  <!-- <Caravela> -->
