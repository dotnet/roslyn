<?xml version="1.0" encoding="utf-8"?>
<Project>

  <!-- Transitive package dependencies must be included in the project as private assets, then we include their output in our own package.-->

  <PropertyGroup>
    <!-- <source-file> is not added because the package already contains file <target-file>. -->
    <MSBuildWarningsAsMessages>$(MSBuildWarningsAsMessages);NU5118</MSBuildWarningsAsMessages>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Composition.AttributedModel" Version="$(SystemCompositionVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
     <PackageReference Include="System.Composition.Convention" Version="$(SystemCompositionVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
    <PackageReference Include="System.Composition.Hosting" Version="$(SystemCompositionVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
    <PackageReference Include="System.Composition.Runtime" Version="$(SystemCompositionVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
    <PackageReference Include="System.Composition.TypedParts" Version="$(SystemCompositionVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
     <PackageReference Include="Humanizer.Core" Version="$(HumanizerCoreVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
     <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="$(MicrosoftBclAsyncInterfacesVersion)" PrivateAssets="all">
            <GeneratePathProperty>true</GeneratePathProperty>
    </PackageReference>
  </ItemGroup>

  <Target Name="InitializeMetalamaCompilerBinArtifacts">

    <ItemGroup>
      <CommonMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)Metalama.Compiler.Interface.TypeForwards\$(Configuration)\netstandard2.0\Metalama.Compiler.Interface.dll" />
      <CommonMetalamaCompilerBinArtifact Include="$(PkgHumanizer_Core)\lib\netstandard1.0\Humanizer.dll"/>
    </ItemGroup>

    <ItemGroup>
      <CoreClrMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net6.0\publish\Microsoft.Bcl.HashCode.dll" />
      <CoreClrMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net6.0\publish\Newtonsoft.Json.dll" />
      <CoreClrMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net6.0\publish\Metalama.Backstage.dll" />
      <CoreClrMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)Microsoft.CodeAnalysis.CSharp.Workspaces\$(Configuration)\netcoreapp3.1\Microsoft.CodeAnalysis.CSharp.Workspaces.dll" />
      <CoreClrMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)Microsoft.CodeAnalysis.Workspaces\$(Configuration)\netcoreapp3.1\Microsoft.CodeAnalysis.Workspaces.dll" />
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_AttributedModel)\lib\netstandard1.0\System.Composition.AttributedModel.dll"/>
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Convention)\lib\netstandard1.0\System.Composition.Convention.dll"/>
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Hosting)\lib\netstandard1.0\System.Composition.Hosting.dll"/>
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Runtime)\lib\netstandard1.0\System.Composition.Runtime.dll"/>
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_TypedParts)\lib\netstandard1.0\System.Composition.TypedParts.dll"/>
      <CoreClrMetalamaCompilerBinArtifact Include="$(PkgMicrosoft_Bcl_AsyncInterfaces)\lib\netstandard2.1\Microsoft.Bcl.AsyncInterfaces.dll"/>
    </ItemGroup>

    <ItemGroup>
      <DesktopMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net472\Microsoft.Bcl.HashCode.dll" />
      <DesktopMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net472\Newtonsoft.Json.dll" />
      <DesktopMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)csc\$(Configuration)\net472\Metalama.Backstage.dll" />
      <DesktopMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)Microsoft.CodeAnalysis.CSharp.Workspaces\$(Configuration)\netstandard2.0\Microsoft.CodeAnalysis.CSharp.Workspaces.dll" />
      <DesktopMetalamaCompilerBinArtifact Include="$(ArtifactsBinDir)Microsoft.CodeAnalysis.Workspaces\$(Configuration)\netstandard2.0\Microsoft.CodeAnalysis.Workspaces.dll" />
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_AttributedModel)\lib\net461\System.Composition.AttributedModel.dll"/>
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Convention)\lib\net461\System.Composition.Convention.dll"/>
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Hosting)\lib\net461\System.Composition.Hosting.dll"/>
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_Runtime)\lib\net461\System.Composition.Runtime.dll"/>
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgSystem_Composition_TypedParts)\lib\net461\System.Composition.TypedParts.dll"/>
      <DesktopMetalamaCompilerBinArtifact Include="$(PkgMicrosoft_Bcl_AsyncInterfaces)\lib\net461\Microsoft.Bcl.AsyncInterfaces.dll"/>
    </ItemGroup>


  </Target>
</Project>
