    <Platforms>x64</Platforms>
    <OutputType>Exe</OutputType>
    <StartupObject>Sub Main</StartupObject>
    <AssemblyName>vbi</AssemblyName>
    <AutoGenerateBindingRedirects>True</AutoGenerateBindingRedirects>
