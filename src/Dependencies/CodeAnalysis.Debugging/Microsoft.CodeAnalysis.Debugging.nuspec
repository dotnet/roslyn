<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="3.3">
    $CommonMetadataElements$
    <dependencies>
      <dependency id="Microsoft.CodeAnalysis.PooledObjects" version="$Version$" />
    </dependencies>
    <contentFiles>
      <files include="**/*.cs" buildAction="Compile" />
    </contentFiles>
  </metadata>
  <files>
    <file src="$SourcePackagePropsFilePath$" target="build" />
    <file src="$ProjectDirectory$\**\*.cs" target="contentFiles/cs/netstandard1.3" />
    <file src="$ProjectDirectory$\**\*.cs" target="contentFiles/cs/net45" />
  </files>
</package>
