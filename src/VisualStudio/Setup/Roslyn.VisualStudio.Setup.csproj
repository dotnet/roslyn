    <NuGetPackageToIncludeInVsix Include="System.Composition.Convention" />
    <NuGetPackageToIncludeInVsix Include="System.Composition.Hosting" />
    <NuGetPackageToIncludeInVsix Include="ICSharpCode.Decompiler" />
